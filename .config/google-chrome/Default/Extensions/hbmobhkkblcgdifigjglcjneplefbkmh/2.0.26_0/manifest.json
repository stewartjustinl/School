{
   "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "images/48_Norton_Ext_Icon.png"
   },
   "chrome_settings_overrides": {
      "search_provider": {
         "encoding": "UTF-8",
         "favicon_url": "https://nortonsafe.search.ask.com/assets/favicon_norton_v1.ico",
         "is_default": true,
         "keyword": "NortonSafe",
         "name": "Norton Safe",
         "search_url": "https://nortonsafe.search.ask.com/web?q={searchTerms}&o=APN11908",
         "suggest_url": "https://ss-sym.search.ask.com/ss?q={searchTerms}&li=ff"
      }
   },
   "content_scripts": [ {
      "js": [ "content.js" ],
      "matches": [ "https://identitysafe.norton.com/*", "http://identitysafe.norton.com/*" ],
      "run_at": "document_start"
   } ],
   "description": "Search safely online by enabling Norton Safe Search as your default search provider for maximum protection on Chrome.",
   "icons": {
      "128": "images/128_Norton_Ext_Icon.png",
      "48": "images/48_Norton_Ext_Icon.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Norton Safe Search as default for Chrome",
   "permissions": [ "tabs", "webRequest", "*://nortonsafe.search.ask.com/", "webRequestBlocking", "storage", "management", "nativeMessaging" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.0.26"
}
